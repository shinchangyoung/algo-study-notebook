package 문자열1.자가진단08;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String str = sc.nextLine();
        if(str.length() <= 100){
            String[] words = str.split(" ");
            for (String word : words) {
                System.out.println(word);
            }

        }


        sc.close();
    }
}
